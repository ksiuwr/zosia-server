server {
       	listen 80 default_server;
       	listen [::]:80 default_server;
        listen 443 ssl;

        ssl_certificate     /etc/letsencrypt/live/staging.zosia.org/cert.pem;
        ssl_certificate_key /etc/letsencrypt/live/staging.zosia.org/privkey.pem;

       	root /srv/zosia-server/html;

       	index index.html;

       	server_name _;

       	location / {
       		try_files $uri $uri/ =404;
       	}

        location /.well-known {
          root /var/www/well-known;
        }
}