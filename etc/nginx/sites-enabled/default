server {
       	listen 80 default_server;
       	listen [::]:80 default_server;


       	root /srv/zosia-server/html;

       	index index.html;

       	server_name _;

       	location / {
       		try_files $uri $uri/ =404;
       	}

        location /.well-known {
          root /var/www/well-known;
        }
}